algoritmo "semnome"

var
num:inteiro
inicio
escreval("Digite um numero entre 1 e 5: ")
leia(num)
escolha num
caso 1
escreval ("UM")

caso 2
escreval ("DOIS")

caso 3
escreval ("TRES")

caso 4
escreval ("Quatro")

caso 5
escreval ("CINCO")
outrocaso
escreval ("numero invalido")
fimescolha
fimalgoritmo



